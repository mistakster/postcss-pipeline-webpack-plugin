# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[test/fixtures/**/*]
insert_final_newline = false

[*.{js,json,yml,css}]
charset = utf-8
indent_style = space
indent_size = 2

[*.md]
charset = utf-8
indent_style = space
indent_size = 4
